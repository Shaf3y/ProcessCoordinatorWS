package com.marisoft.ziba.cep.elements;

import java.util.List;

public class EventType {

	private String eventTypeIdentifier;
	
	/** Denotes whether the event type is a composition of other events or not */
	private boolean composed;
	
	/** Denotes the atom of time from a particular application's point of view */
	private TemporalGranularity granularity;
	
	private EventHeader header;
	
	private List<EventAttribute> payload;
}
