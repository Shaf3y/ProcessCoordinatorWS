package com.marisoft.ziba.cep.enterprise.bpm.events;

import com.marisoft.ziba.cep.Event;
import com.marisoft.ziba.cep.EventBody;
import com.marisoft.ziba.cep.enterprise.bpm.events.bodies.BPStartedEventBody;

public class BProcessEvent extends Event {

	private BPStartedEventBody body;

	/***/
	private static final String BP_STARTED = "BP_STARTED";
	
	public BProcessEvent(Long identity) {
		super(BP_STARTED, identity);
		body = new BPStartedEventBody();
	}

	public EventBody getBody() {
		return body;
	}

}
