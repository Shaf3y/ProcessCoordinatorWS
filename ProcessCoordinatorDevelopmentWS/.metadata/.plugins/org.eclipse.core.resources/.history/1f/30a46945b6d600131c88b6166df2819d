package com.marisoft.ziba.cep.epn;

public class EPNManager {

	private static EPNManager manager = null;
	
	protected EPNManager() {
		
	}
	
	public static EPNManager getManager() {
		if (manager != null) {
			return manager;
		}
		
		// Synchronization for thread safety @ object instantiation
		synchronized (EPNManager.class) {
			if (manager == null) {
				manager = new EPNManager();
			}
		}
		
		return manager;		
	}
}
